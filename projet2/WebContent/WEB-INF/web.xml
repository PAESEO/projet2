<?xml version="1.0" encoding="UTF-8"?>
<web-app 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xmlns="http://java.sun.com/xml/ns/javaee" 
			xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
			id="WebApp_ID" version="3.0">
  	<display-name>projet2</display-name>
  	<welcome-file-list>
	    <welcome-file>index.html</welcome-file>
	    <welcome-file>index.htm</welcome-file>
	    <welcome-file>index.jsp</welcome-file>
	    <welcome-file>default.html</welcome-file>
	    <welcome-file>default.htm</welcome-file>
	    <welcome-file>default.jsp</welcome-file>
  	</welcome-file-list>
  	<servlet>
		<servlet-name>Recuperation</servlet-name>
	    <servlet-class>com.projet.Recuperation</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Recuperation</servlet-name>
	    <url-pattern>/affichageProj</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CalculDistance</servlet-name>
	    <servlet-class>com.projet.CalculDistance</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>CalculDistance</servlet-name>
	    <url-pattern>/calculdistance</url-pattern>
	</servlet-mapping>
</web-app>
